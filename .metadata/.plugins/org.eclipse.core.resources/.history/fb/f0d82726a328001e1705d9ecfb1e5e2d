package com.spring.withoutXML;

public class Manger {

	private String email, team;
	
	/*public User(PersonInterface person) {
		this.person = person;
	}*/
	
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	
	public String getTeam() {
		return team;
	}
	public void setTeam(String team) {
		this.team = team;
	}
}
